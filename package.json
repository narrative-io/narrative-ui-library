{
  "name": "narrative-ui-library",
  "version": "0.1.0",
  "private": false,
	"bugs": {
		"url": "https://github.com/narrative-io/narrative-ui-library/issues"
	},
	"license": "GNU GPL v3.0",
	"main": "dist/narrative-ui-library.umd.min.js",
	"unpkg": "dist/narrative-ui-library.umd.min.js",
	"jsdelivr": "dist/narrative-ui-library.umd.min.js",
	"author": "Narrative, Inc.",
	"description": "The Narrative Data Stream library of user interface components and styles, written for use in Vue.js projects",
	"files": [
		"dist/*",
		"src/*"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/narrative-io/narrative-ui-library"
	},
	"scripts": {
    "serve": "vue-cli-service serve",
		"build": "vue-cli-service build --target lib --name narrative-ui-library src/main.js",
		"prepublishOnly": "$npm_execpath build",
    "lint": "vue-cli-service lint",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "pug": "^3.0.0",
    "pug-plain-loader": "^1.0.0",
    "vuetify": "^2.2.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.25.0",
    "sass-loader": "^7.2.0",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.0.0-rc.1"
	},
	"peerDependencies": {
		"vue": "^2.0.0"
	},
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
	],
	"keywords": [
		"Narrative",
		"Data Stream"
	]
}
